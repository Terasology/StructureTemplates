            {
              "type": "nui:rowLayout",
              "id": "newWidget",
              "layoutInfo": {},
              "contents": [
                {
                  "type": "nui:UICheckbox",
                  "id": "recordBlockRemovalCheckBox",
                  "layoutInfo": {
                    "relativeWidth": 0.1
                  }
                },
                {
                  "type": "nui:UILabel",
                  "layoutInfo": {
                    "useContentWidth": true
                  },
                  "text": "Record block removal"
                }
              ]
            }
          ],
          "layoutInfo": {}
        }
      }
