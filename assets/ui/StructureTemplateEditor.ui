{
  "skin": "engine:default",
  "type": "StructureTemplateEditorScreen",
  "contents": {
    "type": "RelativeLayout",
    "contents": [
      {
        "type": "nui:UIBox",
        "layoutInfo": {
          "use-content-height": true,
          "width": 300,
          "position-vertical-center": {},
          "position-horizontal-center": {}
        },
        "content": {
          "type": "ColumnLayout",
          "verticalSpacing": 4,
          "contents": [
            {
              "type": "UIButton",
              "text": "Edit Structure Template Properties",
              "id": "editTemplatePropertiesButton"
            },
            {
              "type": "UIButton",
              "text": "Make edit region box-shaped",
              "id": "makeBoxShapedButton"
            },
            {
              "type": "UIButton",
              "text": "Selected -\u003e Clipboard",
              "id": "copyToClipboardButton"
